{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake: build",
      "type": "shell",
      "command": "cmake",
      "args": ["--build", "build"]
    },
    {
      "label": "CMake: configure",
      "type": "shell",
      "command": "cmake",
      "args": ["-S", ".", "-B", "build", "-DCMAKE_BUILD_TYPE=Debug"],
      "problemMatcher": ["$gcc"]
    }
  ]
}
